<SSA_Scheduling_SOA_Message xmlns="http://rr.ets.org/v200609/schemas">
	<requestHeader>
		<action>SSA.ExtendHold</action>
		<transactionID>$transactionId$</transactionID>
		<userRoleType>$userRoleType$</userRoleType>
		<source>$source$</source>
		<programCode>$programCode$</programCode>		
		<requestsData>
			<dataElements>
				<name>ExtendHold.SiteId</name>
				<data>
					<string>$siteId$</string>
				</data>
			</dataElements>
			<dataElements>
				<name>Requesting.System</name>
				<data>
					<string>$requestingSystem$</string>
				</data>
			</dataElements>
			<dataElements>
				<name>ExtendHold.ExamId</name>
				<data>
					<string>$testCode$</string>
				</data>
			</dataElements>
			<dataElements>
 				<name>FindSeats.ALTERNATE_FORM</name>
 				<data>
 					<string>$alternateForm$</string>
 				</data>
 			</dataElements> 
		</requestsData>
	</requestHeader>	
	<bookings>
		<booking>
			<holdCode>
				<source>$holdSource$</source>
				<code>$holdCode$</code>
				<duration>$holdDuration$</duration>				
			</holdCode>
		</booking>
	</bookings>
</SSA_Scheduling_SOA_Message>

