<SSA_Scheduling_SOA_Message xmlns="http://rr.ets.org/v200609/schemas">
	<requestHeader>
		<action>SSA.ReserveAppointment</action>
		<transactionID>$transactionId$</transactionID>
		<userRoleType>$userRoleType$</userRoleType>
		<source>$source$</source>
		<programCode>$programCode$</programCode>
		<requestsData>
			<dataElements>
				<name>ReserveAppointment.SiteId</name>
				<data>
					<string>$siteCode$</string>
				</data>
			</dataElements>
			<dataElements>
				<name>ReserveAppointment.TestDate</name>
				<data>
					<date>$testDate$</date>
				</data>
			</dataElements>
			<dataElements>
				<name>ReserveAppointment.ExamId</name>
				<data>
					<string>$testCode$</string>
				</data>
			</dataElements>
			<dataElements>
				<name>ReserveAppointment.TestSession</name>
				<data>
					<string>NA</string>
				</data>
			</dataElements>
			<dataElements>
				<name>ReserveAppointment.TestDuration</name>
				<data>
					<duration>PT$duration$M</duration>
				</data>
			</dataElements>
			<dataElements>
				<name>Requesting.System</name>
				<data>
					<string>$requestingSystem$</string>
				</data>
			</dataElements>			
			<dataElements>
				<name>HasAccommodations</name>
				<data>
					<boolean>$HasAccommodations$</boolean>
				</data>
			</dataElements>
		</requestsData>
	</requestHeader>
	<candidates>
		<candidate>
			<clientCandidateID>$clientCandidateID$</clientCandidateID>
			<firstName>$firstName$</firstName>
			<middleName>$middleName$</middleName>
			<lastName>$lastName$</lastName>
			<birthDate>$birthDate$</birthDate>
			<gender>$gender$</gender>
			<address>
				<addressType>Primary</addressType>
				<addressLine1>$addressLine1$</addressLine1>
				<addressLine2>$addressLine2$</addressLine2>
				<addressLine3>$addressLine3$</addressLine3>
				<addressLine4>$addressLine4$</addressLine4>
				<city>$city$</city>
				<state>$state$</state>
				<country>$country$</country>
				<postalCode>$postalCode$</postalCode>
			</address>
			<phone>
				<phoneType>$phoneType$</phoneType>
				<phoneNumber>$phoneNumber$</phoneNumber>
			</phone>
			$emailAddress$
			$accommodations$
		</candidate>
	</candidates>
	<bookings>
		<booking>
			<clientAppointmentID>$clientAppointmentID$</clientAppointmentID>
			<holdCode>
				<source>$holdSource$</source>
				<code>$holdCode$</code>
				<duration>$holdDuration$</duration>
			</holdCode>
			<slots>
				<slot>
					<siteCode>$siteCode$</siteCode>
					<labCode>$labCode$</labCode>
					<seatCode>$seatCode$</seatCode>
					<startTime>$startTime$</startTime>
				</slot>
			</slots>
		</booking>
	</bookings>
</SSA_Scheduling_SOA_Message>
